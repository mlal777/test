doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(http-equiv="Content-Type", content="text/html; charset=utf-8")
    meta(name='viewport', content='width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no')
    block pageTitle
      -var title;
    title= title
    // Styles
    link(href="./assets/css/fullpage.css", rel="stylesheet")
    link(href="./assets/vendors/bootstrap/bootstrap.min.css", rel="stylesheet")
    link(href="./assets/vendors/slick/slick.css", rel="stylesheet")
    link(href="./assets/vendors/slick/slick-theme.css", rel="stylesheet")
    link(href="./assets/fonts/Icomoon/style.css", rel="stylesheet")
    link(href="./assets/css/styles.css", rel="stylesheet")
    // Favicon
    link(rel="icon", type="image/png", href="./assets/images/favicon.png")
  body
    block active
      -var selected;
    block works
      -var selectedWorks;
    block layout
    
    // Scripts
    script(src="./assets/vendors/jquery/jquery.min.js")
    script(src="./assets/vendors/bootstrap/bootstrap.min.js")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js")
    script(src="https://www.paypalobjects.com/api/checkout.js")
    script(src="./assets/vendors/slick/slick.min.js")
    script(src="./assets/js/fullpage.js")
    script(src="./assets/js/scripts.min.js")